{
  "name": "zendrum-stompblock-client",
  "version": "5.4.0",
  "description": "Web client for Zendrum STOMPBLOCK",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build:prod": "NODE_ENV=production webpack --mode production --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:prod",
    "build:analyze": "NODE_ENV=production webpack --mode production --config webpack.config.analysis.js",
    "analyze": "npm run clean && npm run build:analyze",
    "dev-server": "webpack-dev-server --mode development --config webpack.config.dev.js --hot",
    "dev": "webpack-dev-server --mode development --config webpack.config.dev.js --hot",
    "simulate": "superstatic dist --port 8080",
    "lint": "eslint 'src/**' '*.js'",
    "lint:fix": "npm run lint -- --fix",
    "test": "TZ=America/Los_Angeles NODE_ENV=test jest --env=jsdom",
    "test:watch": "TZ=America/Los_Angeles NODE_ENV=test jest --watchAll",
    "cov": "TZ=America/Los_Angeles NODE_ENV=test jest --coverage",
    "cov:open": "open ./coverage/lcov-report/index.html",
    "cov:watch": "TZ=America/Los_Angeles NODE_ENV=test jest --coverage --watchAll"
  },
  "author": "Darin Kadrioski <darin@nebiru.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:dkadrios/zendrum-stompblock-client.git"
  },
