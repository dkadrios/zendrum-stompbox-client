    "react-dom": "^16.0.0",
    "react-gravatar": "^2.6.3",
    "react-popover": "^0.5.4",
    "react-redux": "^5.0.6",
    "react-toolbox": "^2.0.0-beta.12",
    "react-visible": "^2.0.0",
    "redux": "^3.7.2",
    "redux-form": "^7.1.1",
    "redux-logger": "^3.0.6",
    "redux-midi": "^2.0.2",
    "redux-thunk": "^2.2.0",
    "restify": "^6.0.1",
    "shortid": "^2.2.8"
  },
  "engines": {
    "node": ">=7.0.0"
  }
}
