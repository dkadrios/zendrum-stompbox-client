{
  "name": "zendrum-stompblock-client",
  "version": "0.0.1",
  "description": "Web client for Zendrum STOMPBLOCK",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack -p --config webpack.prod.config.babel.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "open http://localhost:8080/ && webpack-dev-server --config webpack.dev.config.babel.js",
    "lint": "eslint 'src/**' '__tests__/**' '*.js'",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:cov:watch": "jest --coverage --watchAll",
    "cov": "open ./coverage/lcov-report/index.html"
  },
  "author": "Darin Kadrioski <darin@nebiru.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:dkadrios/zendrum-stompblock-client.git"
  },
  "jest": {
    "globals": {
      "__DEV__": true,
      "__TEST__": true,
      "__API__": "http://localhost:3002"
    },
    "setupFiles": [
      "jest-localstorage-mock"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "deep-freeze": "^0.0.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.2",
    "jest": "^21.2.1",
    "jest-localstorage-mock": "^2.0.1",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-each": "^0.10.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.7",
    "postcss-mixins": "^6.1.1",
    "prettier-eslint": "^8.2.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "core-decorators": "^0.20.0",
    "isomorphic-fetch": "^2.2.1",
    "performance-now": "^2.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-canvas-knob": "^0.5.0",
