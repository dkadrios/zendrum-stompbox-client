{
  "name": "zendrum-stompblock-client",
  "version": "0.0.1",
  "description": "Web client for Zendrum STOMPBLOCK",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack":
      "NODE_ENV=production webpack -p --config webpack.prod.config.babel.js",
    "build": "npm run clean && npm run build:webpack",
    "start":
      "open http://localhost:8080/ && webpack-dev-server --config webpack.dev.config.babel.js",
    "lint": "eslint 'src/**' '__tests__/**' '*.js'",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:cov:watch": "jest --coverage --watchAll",
    "cov": "open ./coverage/lcov-report/index.html",
    "flow:cov":
      "flow-coverage-report -i 'src/**/*.js' -i 'src/**/*.jsx' -x 'src/test/**' -t html -t json -t text"
  },
  "author": "Darin Kadrioski <darin@nebiru.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:dkadrios/zendrum-stompblock-client.git"
  },
  "jest": {
    "globals": {
      "__DEV__": true,
      "__TEST__": true
    }
  },
